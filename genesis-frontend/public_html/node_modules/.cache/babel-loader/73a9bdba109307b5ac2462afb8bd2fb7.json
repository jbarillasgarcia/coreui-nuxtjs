}\n      }\n      let focused = false\n      component.$children.some((child) => {\n        focused = this.findFirstError(child)\n        return focused\n      })\n\n      return focused\n    },\n    validate() {\n      this.$v.$touch()\n      this.$nextTick(() => this.findFirstError())\n      return this.isValid\n    }\n  }\n}\n</script>\n\n<style scoped>\n.btn.disabled {\n  cursor: auto;\n}\n</style>\n"],"sourceRoot":"src/views/forms"}]}